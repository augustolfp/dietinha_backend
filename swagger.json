{
    "openapi": "3.0.0",
    "info": {
        "title": "Dietinha Calories Tracker",
        "description": "This API was built to track daily macronutrients intake of users, and provides a search mechanism on data provided by TACO-Unicamp. You need a valid Access Token (provided by Firebase Auth) to access protected Endpoints.\n\nTo generate an Access Token, you can use the Standalone App:\n- [Dietinha Standalone Authentication app](https://api.dietinha.co/v2/standalone-auth)",
        "contact": {
            "email": "augustolfp@gmail.com"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "/v2"
        }
    ],
    "paths": {
        "/daily-log": {
            "post": {
                "tags": ["Daily-Logs"],
                "summary": "Insert new Daily Log",
                "description": "The user has to create a Daily Log providing his unique Id and the date of the log (not necessarily the current date). The user will be identified through the Auth Token Payload.",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/SentDailyLog"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "201": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ReceivedDailyLog"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Missing or Invalid Credentials"
                    },
                    "403": {
                        "description": "User already have a Daily-log in provided Date"
                    },
                    "422": {
                        "description": "Invalid or incomplete Request Body"
                    }
                }
            },
            "get": {
                "tags": ["Daily-Logs"],
                "summary": "Get user's Daily Logs",
                "description": "Returns a summary of calories intake by day, for specific user",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/ReceivedDailyLogArray"
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Missing or Invalid Credentials"
                    }
                }
            }
        },
        "/daily-log/{daily-log-id}": {
            "get": {
                "tags": ["Daily-Logs"],
                "summary": "Get a detailed description of a specific user daily-log",
                "description": "",
                "parameters": [
                    {
                        "name": "daily-log-id",
                        "in": "path",
                        "description": "Id of the daily-log",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ReceivedDetailedDailyLog"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Missing or Invalid Credentials"
                    },
                    "404": {
                        "description": "User does not have a daily-log with provided id"
                    }
                }
            },
            "delete": {
                "tags": ["Daily-Logs"],
                "summary": "Delete one daily-log",
                "description": "All meals and ingredients related to this meal will also be deleted",
                "parameters": [
                    {
                        "name": "daily-log-id",
                        "in": "path",
                        "description": "Id of the daily-log to delete",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Missing or Invalid Credentials"
                    },
                    "404": {
                        "description": "User does not have a daily-log with provided id"
                    }
                }
            }
        },
        "/meal": {
            "post": {
                "tags": ["Meals"],
                "summary": "Insert new Meal, as a part of a Daily-Log",
                "description": "",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/SentMeal"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "201": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ReceivedMeal"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Missing or Invalid Credentials"
                    },
                    "404": {
                        "description": "User does not have a daily-log with provided id"
                    },
                    "422": {
                        "description": "Invalid or incomplete Request Body"
                    }
                }
            }
        },
        "/meal/{mealId}": {
            "delete": {
                "tags": ["Meals"],
                "summary": "Delete one meal",
                "description": "All ingredients related to this meal will also be deleted",
                "parameters": [
                    {
                        "name": "mealId",
                        "in": "path",
                        "description": "Id of the meal to delete",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Missing or Invalid Credentials"
                    },
                    "404": {
                        "description": "User does not have a meal with provided id"
                    }
                }
            }
        },
        "/ingredient": {
            "post": {
                "tags": ["Ingredients"],
                "summary": "Insert new Ingredient, as a part of a Meal",
                "description": "",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/SentIngredient"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "201": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Ingredient"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Missing or Invalid Credentials"
                    },
                    "404": {
                        "description": "User does not have a meal with provided id"
                    },
                    "422": {
                        "description": "Invalid or incomplete Request Body"
                    }
                }
            }
        },
        "/ingredient/{ingredientId}": {
            "delete": {
                "tags": ["Ingredients"],
                "summary": "Delete one ingredient",
                "description": "",
                "parameters": [
                    {
                        "name": "ingredientId",
                        "in": "path",
                        "description": "Id of the ingredient to delete",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation"
                    },
                    "401": {
                        "description": "Missing or Invalid Credentials"
                    },
                    "404": {
                        "description": "User does not have a ingredient with provided id"
                    }
                }
            }
        },
        "/datatable/search/{searchTerm}": {
            "get": {
                "tags": ["Data Table (Food Database)"],
                "summary": "Search for food nutrient profiles, from TACO table and CustomIngredients table",
                "description": "",
                "parameters": [
                    {
                        "name": "searchTerm",
                        "in": "path",
                        "description": "The name (or part of the name) of the ingredient. Ex: ovo",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/TacoTableItem"
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Missing or Invalid Credentials"
                    }
                }
            }
        },
        "/datatable/custom": {
            "post": {
                "tags": ["Data Table (Food Database)"],
                "summary": "Add a private custom food nutrition profile",
                "description": "",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/SentCustomLog"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "201": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ReceivedCustomLog"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Missing or Invalid Credentials"
                    },
                    "403": {
                        "description": "User already have a Nutrition Log with provided description"
                    },
                    "422": {
                        "description": "Invalid or incomplete Request Body"
                    }
                }
            }
        }
    },
    "components": {
