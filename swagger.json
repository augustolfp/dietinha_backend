            "SentDailyLog": {
                "type": "object",
                "required": ["date", "caloriesTarget", "proteinsTarget"],
                "properties": {
                    "date": {
                        "type": "string",
                        "example": "yyyy-MM-dd"
                    },
                    "notes": {
                        "type": "string",
                        "example": "It was a busy day, couldn't cook anything myself"
                    },
                    "caloriesTarget": {
                        "type": "number",
                        "example": 2500
                    },
                    "proteinsTarget": {
                        "type": "number",
                        "example": 130
                    }
                }
            },
            "ReceivedDailyLog": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "format": "uuid"
                    },
